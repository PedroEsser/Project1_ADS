<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

<servlet>
	<servlet-name>LeTestServlet</servlet-name>
	<display-name>LeTestSelvTes</display-name>
	<description></description>
	<servlet-class>servlets.LeTestServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>LoginServlet</servlet-name>
	<display-name>LoginServlet</display-name>
	<description></description>
	<servlet-class>servlets.LoginServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>CreateClassServlet</servlet-name>
	<display-name>CreateClassServlet</display-name>
	<description></description>
	<servlet-class>servlets.CreateClassServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>LeTestServlet</servlet-name>
	<url-pattern>/leTest</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>LoginServlet</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>CreateClassServlet</servlet-name>
	<url-pattern>/create_class</url-pattern>
</servlet-mapping>

<welcome-file-list>
	<welcome-file>taxonomy.jsp</welcome-file>
</welcome-file-list>

</web-app>
